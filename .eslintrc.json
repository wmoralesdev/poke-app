{
    "extends": "airbnb",
    "rules": {
        "indent": [2, 4],
        "no-restricted-exports": 0,

        "react/jsx-indent": [2, 4],
        "react/jsx-filename-extension": 0,
        "react/function-component-definition": [ 2, {
            "namedComponents": "arrow-function",
            "unnamedComponents": "arrow-function"
        }],
        "react/prop-types": 0
    }
}